{
  "name": "wayback-dl",
  "version": "1.0.0",
  "description": "A beautiful Wayback Machine downloader with parallel downloading support",
  "type": "module",
  "scripts": {
    "start": "deno run --allow-net --allow-read --allow-write --allow-env src/main.ts",
    "dev": "deno run --watch --allow-net --allow-read --allow-write --allow-env src/main.ts",
    "lint": "deno lint",
    "fmt": "deno fmt",
    "check": "deno check"
  },
  "keywords": [
    "wayback-machine",
    "archive",
    "downloader",
    "deno"
  ],
  "license": "MIT"
}
